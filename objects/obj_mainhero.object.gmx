if (position_meeting(x, y + 1, obj_platform) and (!onPlatform))
{    
    if (hero_state != "jump")
    {
        hero_state = "falling";
    }
}

if (((!place_free(x, y + 1)) or onPlatform) and hero_state == "falling")
{    
    hero_state = "idle";
}

if (hero_state == "falling")
{
    if (y + jump_speed &lt; 1936)
    {
        y += jump_speed;
    }
    else
    {
        y += 1936 - y;
    }
    image_speed = 0;
    sprite_index = spr_hero_jump;
    image_index = 4;
}

if (hero_state == "idle")
{
    image_speed = 0.04;
    sprite_index = spr_hero_idle;
}

if (hero_state == "left")
{
    image_speed = 0.2;
    if(!place_free(x, y + 1) or onPlatform) 
    {
        sprite_index = spr_hero_walk;
    }
    if (!audio_is_playing(walk_sound))
    {
        audio_play_sound(walk_sound, 10, false);
    }
}

if (hero_state == "right")
{
    image_speed = 0.2;
    if(!place_free(x, y + 1) or onPlatform) 
    {
        sprite_index = spr_hero_walk;
    }
    if (!audio_is_playing(walk_sound))
    {
        audio_play_sound(walk_sound, 10, false);
    }
}

if (hero_state == "jump")
{
    if (vert_speed &lt; 14)
    {
        vert_speed += grav;
    }
    
    image_speed = 0.3;
    sprite_index = spr_hero_jump;
    
    if (image_index &gt;= 4 and !(!place_free(x, y + 1) or onPlatform))
    {
        image_index = 4;
    }
    
    if (image_index &gt; 4 and image_index &lt; 5 and (!place_free(x, y + 1) or onPlatform))
    {
        image_index = 5;
        jumped = false;
    }
    
    if (image_index &gt;= 5.7)
    {
        hero_state = "idle";
    }
    
    if (!place_free(x, y + 1) or onPlatform)
    {
        if (!jumped)
        {
            vert_speed = 0;
        }
        else
        {
            vert_speed = -jump_speed;
        }
    }

    if (place_free(x, y + 1) and !onPlatform)
    {
        audio_stop_sound(walk_sound);
    }

    y += vert_speed;
}

if (hero_state == "half_damaged")
{
    if (alarm[0] == -1) alarm[0] = 5;
}

if (hero_state == "full_damaged")
{
    if (alarm[0] == -1) alarm[0] = 5;
    image_speed = 0.4;
    sprite_index = spr_hero_damage;    
}

if (maxhealth &gt; 3)
{
    image_blend = c_white;
}
else
{
    if (maxhealth &gt; 1)
    {
        image_blend = color_warning;
    }
    else
    {
        image_blend = color_danger;
    }
