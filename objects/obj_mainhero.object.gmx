    if(!place_free(x, y + 1)){
        if(!jumped)
            vert_speed = 0;
        else
            vert_speed = -jump_speed;
    }

    if(place_free(x, y + 1))
        audio_stop_sound(walk_sound);

    y += vert_speed;
}

