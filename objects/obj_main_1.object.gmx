        text = ds_list_find_value(theme_info, i);
    }
    else
    {
        text = "Сейчас данная тема не доступна";
    }
        
    draw_set_font(Runic_46);
    
    if (coords[i, 0] - 10 &lt; triangle_x3 and coords[i, 0] + 10 &gt; triangle_x3 and coords[i, 1] &gt; triangle_y3 and i &lt; ds_list_size(theme_info)) //If scenario-number is under the triangle
    {
