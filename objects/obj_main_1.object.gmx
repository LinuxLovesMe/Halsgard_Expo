if (point_in_circle(mouse_x, mouse_y, center_x, center_y, sprite_get_width(spr_main_3) / 2))
{
    for (i = 0; i &lt; ds_list_size(theme_info); i++)
    {
        if (coords[i, 0] - 10 &lt; triangle_x3 and coords[i, 0] + 10 &gt; triangle_x3 and coords[i, 1] &gt; triangle_y3)
        {
            filename = "dark_forest_dev/Dialogue_2_" + string(i + 1) + ".txt";
            
            //Working with themes' number iterators
            
            if (i != 0 and i != 2 and i != 3 and i != 4) global.character[9] -= 1; //Change number of local level AP amount
            
            if (i != 3 and i != 4)
            {
                if (!instance_exists(obj_dialogue_2)) instance_create(0, 0, obj_dialogue_2);
                instance_deactivate_object(obj_main_1);
            }
            else
            {
                var value = item_check_value("Halsvord");
                
                if (i == 3 and value &gt;= 50)
                {
                    if (!instance_exists(obj_dialogue_2)) instance_create(0, 0, obj_dialogue_2);
                    instance_deactivate_object(obj_main_1);
                }
                else if (i == 4 and value &gt;= 150)
                {
                    item_set_value("Halsvord", value - 150);
                    global.garage_secret = true;
                    if (!instance_exists(obj_dialogue_2)) instance_create(0, 0, obj_dialogue_2);
                    instance_deactivate_object(obj_main_1);
                }
            }
            break;
        }
    }
}
